<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Verificador.
    -->
    <changeSet id="20201125015411-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="macroproceso_id"
                                 baseTableName="verificador"
                                 constraintName="fk_verificador_macroproceso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="macroproceso"/>

        <addForeignKeyConstraint baseColumnNames="proceso_id"
                                 baseTableName="verificador"
                                 constraintName="fk_verificador_proceso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proceso"/>

        <addForeignKeyConstraint baseColumnNames="subproceso_id"
                                 baseTableName="verificador"
                                 constraintName="fk_verificador_subproceso_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subproceso"/>

    </changeSet>
</databaseChangeLog>
